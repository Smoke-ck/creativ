  function userInt(entereDate) {
	const promptUser = parseInt(prompt(entereDate));

	return promptUser;
}

const num = userInt;

function res (num1, num2) {
 if(isNaN(num1 && num2)) {
 console.log('Некорректный ввод!')
 } else {
 console.log(parseInt(num1.toString(num2)))
 }
}

function main (num) {

  const first = userInt("first number")

  const second = userInt("second number")
  
  
    return res(first, second);
}
main(num);
