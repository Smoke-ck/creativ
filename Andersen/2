 function userInt(entereDate) {
	const promptUser = parseInt(prompt(entereDate));
  const askUser = !isNaN(promptUser) ? promptUser :console.log('Некорректный ввод!')
	return askUser;
}

const num = userInt;

const result1 = (num1, num2) => {
const sum = num1 + num2;
const division = num1 / num2;
 console.log(` Ответ : ${sum}.${division}`)
}

function main (num) {

  const first = userInt("first number")
  if(isNaN(first)) {
  	return
  }
  const second = userInt("second number")
  
   if(isNaN(second)) {
  	return
  }
    return result1(first, second);
}
main(num);
